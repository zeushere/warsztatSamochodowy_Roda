<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="config.MainController">
    <children>
        <HBox prefHeight="45.0" prefWidth="1200.0">
            <children>
                <Button mnemonicParsing="false" onAction="#uslugaTable" prefWidth="150.0" text="Usługa">
                    <HBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#wlascicelTable" prefWidth="150.0" text="Właściciel">
                    <HBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#naprawaTable" prefWidth="150.0" text="Naprawa">
                    <HBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#update" prefWidth="150.0" text="Aktualizacja">
                    <HBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#search" prefWidth="150.0" text="Wyszukiwanie">
                    <HBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </HBox.margin>
                </Button>
            <Button mnemonicParsing="false" onAction="#add" prefWidth="150.0" text="Dodawanie">
               <HBox.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#del" prefWidth="150.0" text="Usuwanie">
               <HBox.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </HBox.margin>
            </Button>
            </children>
        </HBox>
        <Pane fx:id="wyswietl" layoutY="65.0" prefHeight="372.0" prefWidth="1200.0" />
    </children>
</AnchorPane>
