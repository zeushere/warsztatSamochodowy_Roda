            <Text layoutX="476.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wpisz co chcesz wyszukać" />
            <TextField fx:id="searchTextField" layoutX="471.0" layoutY="38.0" />
            <Button fx:id="searchFromTableButton" layoutX="672.0" layoutY="38.0" mnemonicParsing="false" onAction="#searchFromTableAction" text="Szukaj" />
