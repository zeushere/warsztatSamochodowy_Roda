<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="313.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="service.updateService.updateNaprawa">
    <children>
        <Pane prefHeight="313.0" prefWidth="478.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Text layoutX="130.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Wybierz naprawę do aktualizacji"/>
                <ComboBox fx:id="selectNaprawa" layoutX="127.0" layoutY="84.0" onAction="#selectNaprawaAction"
                          prefWidth="150.0"/>
                <Text layoutX="127.0" layoutY="131.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Koszt naprawy"/>
                <TextField fx:id="naprawaUpdateKosztNaprawy" layoutX="129.0" layoutY="144.0"/>
                <CheckBox fx:id="updateCheckboxKosztNaprawy" layoutX="294.0" layoutY="148.0" mnemonicParsing="false"
                          onAction="#checkBoxKosztNaprawy"/>
                <Text layoutX="129.0" layoutY="198.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Data naprawy"/>
                <DatePicker fx:id="naprawaUpdateDataNaprawy" layoutX="128.0" layoutY="212.0" promptText="dd.mm.rrrr"/>
                <Button fx:id="naprawaUpdateButton" layoutX="141.0" layoutY="251.0" mnemonicParsing="false"
                        onAction="#naprawaUpdate" text="Zaktualizuj naprawę"/>
                <CheckBox fx:id="updateCheckboxDataNaprawy" layoutX="349.0" layoutY="216.0" mnemonicParsing="false"
                          onAction="#checkBoxDataNaprawy"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
