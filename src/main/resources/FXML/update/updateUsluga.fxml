<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="config.update.updateUsluga">
   <children>
      <Pane prefHeight="400.0" prefWidth="400.0">
         <children>
            <TextField fx:id="uslugaUpdateNazwa" layoutX="123.0" layoutY="141.0" />
            <TextField fx:id="uslugaUpdateRodzaj" layoutX="122.0" layoutY="200.0" />
            <Text layoutX="126.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nazwa usługi" />
            <Text layoutX="126.0" layoutY="189.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Rodzaj usługi" />
            <Button fx:id="updateUslugaButton" layoutX="126.0" layoutY="249.0" mnemonicParsing="false" onAction="#updateUsluga" text="Zaktualizuj usługę" />
            <ComboBox fx:id="selectUsluga" layoutX="121.0" layoutY="58.0" onAction="#selectUslugaAction" prefWidth="150.0" />
            <CheckBox fx:id="updateCheckboxNazwaUslugi" layoutX="279.0" layoutY="145.0" mnemonicParsing="false" onAction="#checkBoxNazwaUslugi" />
            <CheckBox fx:id="updateCheckboxRodzajUslugi" layoutX="279.0" layoutY="204.0" mnemonicParsing="false" onAction="#checkBoxRodzajUslugi" />
            <Text layoutX="126.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wybierz usługę do aktualizacji" />
         </children>
      </Pane>
   </children>
</AnchorPane>
